{
  "overrides": {
    "MuiButton": {
      "root": {
        "border-radius": "0",
        "background": "black",
        "&:hover": {
          "background-color": "black"
        }
      },
      "text": {
        "color": "white",
        "padding": "6px 16px"
      }
    }
  },
  "palette": {
    "common": { "black": "#000", "white": "#fff" },
    "background": {
      "paper": "rgba(255, 255, 255, 1)"
    },
    "primary": {
      "light": "rgba(60, 209, 188, 1)",
      "main": "rgba(20, 160, 140, 1)",
      "dark": "rgba(2, 114, 98, 1)",
      "contrastText": "#fff"
    },
    "secondary": {
      "light": "rgba(255, 43, 101, 1)",
      "main": "rgba(231, 2, 58, 1)",
      "dark": "rgba(143, 1, 41, 1)",
      "contrastText": "#fff"
    },
    "error": {
      "light": "#e57373",
      "main": "#f44336",
      "dark": "#d32f2f",
      "contrastText": "#fff"
    },
    "text": {
      "primary": "rgba(0, 0, 0, 0.87)",
      "disabled": "rgba(0, 0, 0, 0.38)",
      "hint": "rgba(0, 0, 0, 0.54)"
    },
    "neutral": {
      "main": "#000",
      "contrastText": "#fff"
    }
  }
